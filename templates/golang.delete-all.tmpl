{{- define "signature" -}}
Delete{{ .Suffix }}({{ params .Args }}) (count int64, err error)
{{- end -}}

{{- define "body" -}}{
	const __stmt=`{{ .SQL }}`
	__prepared, __replace, err := obj.prepare(__stmt)
	if err != nil {
        return 0, WrapErr(err)
	}
	defer __replace()
	obj.logStmt(__stmt, {{ args .Args }})

    __res, err := __prepared.Exec({{ args .Args }})
	if err != nil {
        return 0, WrapErr(err)
	}
	count, err = __res.RowsAffected()
	if err != nil {
        return 0, WrapErr(err)
	}

    return count, nil
}
{{ end -}}
