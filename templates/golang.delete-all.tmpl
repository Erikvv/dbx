{{- define "signature" -}}
Delete{{ .Suffix }}({{ param .Args }}) (count int64, err error)
{{- end -}}

{{- define "body" -}}{
	const __stmt=`{{ .SQL }}`
	obj.logStmt(__stmt, {{ arg .Args }})

	__res, err := obj.impl.Exec(__stmt, {{ arg .Args }})
	if err != nil {
		return 0, WrapErr(err)
	}

	count, err = __res.RowsAffected()
	if err != nil {
		return 0, WrapErr(err)
	}

	return count, nil
}
{{ end -}}
