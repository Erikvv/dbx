{{- define "signature" -}}
Delete{{ .Suffix }}({{ param .Args }}) (deleted bool, err error)
{{- end -}}

{{- define "body" -}}{
	const __stmt=`{{ .SQL }}`
	obj.logStmt(__stmt, {{ arg .Args }})

	__res, err := obj.driver.Exec(__stmt, {{ arg .Args }})
	if err != nil {
		return false, WrapErr(err)
	}

	__count, err := __res.RowsAffected()
	if err != nil {
		return false, WrapErr(err)
	}

	return __count > 0, nil
}
{{ end -}}
