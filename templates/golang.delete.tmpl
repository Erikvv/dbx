{{- define "signature" -}}
Delete{{ .Suffix }}({{ params .Args }}) (deleted bool, err error)
{{- end -}}

{{- define "body" -}}{
	const __stmt=`{{ .SQL }}`
	__prepared, __replace, err := obj.prepare(__stmt)
	if err != nil {
        return false, WrapErr(err)
	}
	defer __replace()
	obj.logStmt(__stmt, {{ args .Args }})

    __res, err := __prepared.Exec({{ args .Args }})
	if err != nil {
        return false, WrapErr(err)
	}
	__count, err := __res.RowsAffected()
	if err != nil {
        return false, WrapErr(err)
	}

    return __count > 0, nil
}
{{- end -}}

func (obj *{{ .Dialect }}DB) {{ template "signature" . }} {{ template "body" . }}

func (obj *{{ .Dialect }}Tx) {{ template "signature" . }} {{ template "body" . }}
